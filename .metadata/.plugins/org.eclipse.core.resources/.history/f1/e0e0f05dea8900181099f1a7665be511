grammar com.ufcg.compiler.Go with org.eclipse.xtext.common.Terminals

generate go "http://www.ufcg.com/compiler/Go"

Init:
	VarDecl+=VarDecl*
;

Go:
	VarDecl
;

VarDecl:
	TIPO Terminals::ID IGUAL EXPRESSAO
;

TIPO:
	"var" | "const"
;

IGUAL:
	"="
;

EXPRESSAO:
	NUMERO
;

NUMERO:
	Terminals::INT
;

STRING_DECL:
	ASPAS Terminals::STRING ASPAS
;

ASPAS:
	'"'
;